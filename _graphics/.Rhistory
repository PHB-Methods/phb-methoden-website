library(tidyverse)
theme_void() %+replace% theme(
legend.position="none",
panel.background = element_rect(fill="#000000"),
plot.margin=unit(c(-0.5, -1, -0.5, -1), 'cm')
) -> th
pal_colours <- c("#ffffff", "#0067A2", "#000000", "#000000")
pal_values <- c(0, 0.1, 0.4, 1)
#| fig.width: 8
#| fig.height: 4
n <- 200000
tibble(
x = runif(n, min = 0, max = 10),
y = runif(n, min = 0, max = 0.3),
d = y - dnorm(x, mean = 5, sd = 2)
) -> dots
dots %>% filter(d >= 0) %>%
ggplot(aes(x = x, y = y, color = d)) +
geom_point(alpha = 0.2, size = 0.01) +
scale_colour_gradientn(colors = pal_colours, values = pal_values) +
th -> plot
plot
